/* pages/fundArchive/fundArchive.wxss */
.table {
  border: 0px solid darkgray;
  font-size: 12px;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  align-items: center;
}

.td {
  width: 100%;
  justify-content: center;
  text-align: center;
}

.bg-w {
  background: snow;
}

.th {
  width: 100%;
  justify-content: center;
  background: #fff;
  color: #323233;
  font-size: 13px;
  display: flex;
  height: 2rem;
  align-items: center;
}

.td1, .th1 {
  width: 40%;
}

.td2, .th2{
  width: 60%;
}

